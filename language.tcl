proc gs { str } {
    global tr_ls
    if { ![info exists TuxRacerConfig::language] } {
	namespace eval TuxRacerConfig {
	    set language english
	}
    }
    if { ![info exists tr_ls($TuxRacerConfig::language,$str)] } {
	return "undef" 
    }
    return $tr_ls($TuxRacerConfig::language,$str)
}

proc check_names {} {
    global tr_ls
    foreach i [array names tr_ls english*] {
	foreach l {french italian german spanish} {
	    regsub english $i $l index
	    if { ![info exists tr_ls($index)] } {
		puts "String undefined: $index"
	    }
	}
    }
}
